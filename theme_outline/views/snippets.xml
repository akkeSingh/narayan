    <template id="latest_posts_template">
        <div t-foreach="posts" class="col-12 col-sm-6 col-md-4 media mt-3 s_latest_posts_post" t-as="p">
            <t t-set="_record" t-value="p"/>
            <a t-attf-href="/blog/#{p.blog_id.id}/post/#{p.id}">
                <span class="o_record_cover_image"/>
                <div class="media-body pb-2">
                    <h6 class="h_tag" t-esc="_record.name"/>
                    <p class="p_tag text-muted" t-field="_record.subtitle"/>
                </div>
                <div class="media-body author_detail d-flex pb-2">
                    <span class="image" t-esc="_record.author_avatar" t-options="{'widget':'image'}"/>
                    <span class="detail">
                        <span class="text-muted p_date" t-esc="_record.published_date"/>
                        <span class="name" t-esc="_record.author_id.name"/>
                    </span>
                </div>
            </a>
        </div>
    </template>  
    <template id="latest_posts_2_template">
        <div t-foreach="posts" class="col-md-4 col-sm-6 col-12 pt8 pb8 s_latest_posts_post"  t-as="p" >
            <div class="item-box-blog">
                <div class="item-box-blog-image">
                    <div class="item-box-blog-date">
                        <span t-esc="p.published_date" class="mon" t-options="{'widget':'date','format':'MMMM dd'}"/>
                    </div>
                    <figure>
                        <a class="s_latest_posts_post_cover position-relative d-flex flex-column shadow-sm overflow-hidden rounded text-decoration-none" t-attf-href="/blog/#{p.blog_id.id}/post/#{p.id}">
                            <t t-call="website.record_cover">
                                <t t-set="_record" t-value="p"/>
                                <t t-set="use_filters" t-value="True"/>
                                <t t-set="additionnal_classes" t-value="'h-100 w-100 bg-600 position-absolute'"/>
                            </t>
                        </a>
                    </figure>
                </div>
                <div class="item-box-blog-body">
                    <t t-set="_record" t-value="p"/>
                    <div class="item-box-blog-heading">
                        <a t-attf-href="/blog/#{p.blog_id.id}/post/#{p.id}" tabindex="0">
                            <h5 class="h_tag" t-esc="p.name"/>
                        </a>
                    </div>
                    <!--Data-->
                    <div class="item-box-blog-data" style="padding:15px;">
                        <p>
                            <i class="fa fa-user-o"/>
                            <span class="uname" t-esc="_record.author_id.name"/>
                            <i class="fa fa-comments-o"/>
                            <span class="visits">
                                visits(<t t-esc="_record.visits"/>)
                            </span>
                        </p>
                    </div>
                    <!--Text-->
                    <div class="item-box-blog-text">
                        <p t-field="_record.subtitle"></p>
                    </div>
                    <!-- Read More Button-->
                    <div class="item-box-blog-btn">
                        <a class="btn read" t-attf-href="/blog/#{p.blog_id.id}/post/#{p.id}">Read More</a>
                    </div>
                </div>
            </div>
        </div>
    </template>   
    <template id="latest_posts_3_template">
        <figure t-foreach="posts" t-as="p" class="col-12 col-md-6 col-xl-4 my-3 s_latest_posts_post">
            <a class="s_latest_posts_post_cover position-relative d-flex flex-column shadow-sm overflow-hidden rounded text-decoration-none" t-attf-href="/blog/#{p.blog_id.id}/post/#{p.id}">
                <t t-call="website.record_cover">
                    <t t-set="_record" t-value="p"/>
                    <t t-set="use_filters" t-value="True"/>
                    <t t-set="additionnal_classes" t-value="'h-100 w-100 bg-600 position-absolute'"/>
                </t>

                <figcaption class="text-center w-100 h-100 px-3 d-flex flex-column flex-grow-1">
                    <div class="s_latest_posts_post_title text-white" t-field="p.name"/>
                    <div class="s_latest_posts_post_subtitle text-white" t-field="p.subtitle"/>
                </figcaption>

                <div class="s_latest_posts_loader d-flex align-items-center justify-content-center bg-200">
                    <div class="d-none rounded-circle bg-black-25 w-25 h-25"/>
                </div>
            </a>
        </figure>
    </template>
